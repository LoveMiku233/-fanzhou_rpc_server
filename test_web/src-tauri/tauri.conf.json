{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "fanzhou RPC Debug Tool",
  "version": "1.0.0",
  "identifier": "com.fanzhou.rpc-debug-tool",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 700,
        "resizable": true,
        "title": "fanzhou RPC Debug Tool",
        "width": 600,
        "center": true,
        "url": "launch.html"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "externalBin": [
      "bin/websocat"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/64x64.png",
      "icons/128x128.png",
      "icons/256x256.png",
      "icons/512x512.png",
      "icons/icon.ico",
      "icons/icon.png"
    ],
    "longDescription": "泛舟RPC服务器调试工具",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "泛舟RPC调试工具",
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
